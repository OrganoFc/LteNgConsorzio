{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "lte-angular"
  },
  "apps": [
    {
      "root": "lteAngular/src",
      "outDir": "public/dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "plugin/bootstrap/css/bootstrap.min.css",
        "plugin/bootstrap/font-awesome-4.5.0/css/font-awesome.min.css",
        "plugin/ionicons-2.0.1/css/ionicons.min.css",
        "plugin/dist/css/AdminLTE.min.css",
        "plugin/dist/css/skins/_all-skins.min.css",
        "plugin/plugins/iCheck/flat/blue.css",
        "plugin/plugins/morris/morris.css",
        "plugin/plugins/jvectormap/jquery-jvectormap-1.2.2.css",
        "plugin/plugins/datepicker/datepicker3.css",
        "plugin/plugins/daterangepicker/daterangepicker.css",
        "plugin/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css"
      ],
      "scripts": [
        "plugin/plugins/jQuery/jquery-2.2.3.min.js",
        "plugin/plugins/jQueryUI/jquery-ui.min.js",
        "plugin/risolveConflictoJQuery.js",
        "plugin/bootstrap/js/bootstrap.min.js",
        "plugin/plugins/raphael/raphael-min.js",
        "plugin/plugins/morris/morris.min.js",
        "plugin/plugins/sparkline/jquery.sparkline.min.js",
        "plugin/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js",
        "plugin/plugins/jvectormap/jquery-jvectormap-world-mill-en.js",
        "plugin/plugins/knob/jquery.knob.js",
        "plugin/plugins/moment.js/moment.min.js",
        "plugin/plugins/daterangepicker/daterangepicker.js",
        "plugin/plugins/datepicker/bootstrap-datepicker.js",
        "plugin/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js",
        "plugin/plugins/slimScroll/jquery.slimscroll.min.js",
        "plugin/plugins/fastclick/fastclick.js",
        "plugin/dist/js/app.min.js",
        "plugin/dist/js/pages/dashboard.js",
        "plugin/dist/js/demo.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "lteAngular/src/tsconfig.app.json"
    },
    {
      "project": "lteAngular/src/tsconfig.spec.json"
    },
    {
      "project": "lteAngular/e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
